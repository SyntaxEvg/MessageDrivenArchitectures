{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "MinimumLevel": "Information"
      },
      {
        "Name": "File",
        "MinimumLevel": "Information"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },

  "AppSettings": {
    "ConsoleTitle": "KitchenLogs"
  },

  "RabbitMqSettings": {
    "Host": "localhost", //docker 
    "User": "admin",
    "Password": "admin",
    "VirtualHost": "ready",
    "Port": 15672
  }
}